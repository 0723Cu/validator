mml.peschema = mml.maligngroup.qname | mml.malignmark.qname
mml.malignmark.qname = element malignmark { empty, mml.att-edge }
mml.maligngroup.qname =
    element maligngroup { empty, mml.Common.attrib, mml.att-groupalign }
mml.mglyph.qname =
    element mglyph {
        empty, mml.att-alt, mml.att-fontfamily, mml.att-index
    }
