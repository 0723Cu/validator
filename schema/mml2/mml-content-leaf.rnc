mml.ctoken = mml.csymbol.qname | mml.ci.qname | mml.cn.qname
mml.ci.qname =
    element ci {
        (mml.MathMLCharacters | mml.PresInCont)*,
        mml.Common.attrib,
        mml.att-type,
        mml.att-definition,
        mml.att-encoding
    }
mml.csymbol.qname =
    element csymbol {
        (mml.MathMLCharacters | mml.PresInCont)*,
        mml.Common.attrib,
        mml.att-type,
        mml.att-definition,
        mml.att-encoding
    }
mml.cn.qname =
    element cn {
        (mml.MathMLCharacters | mml.PresInCont | mml.sep.qname)*,
        mml.Common.attrib,
        mml.att-type,
        mml.att-base,
        mml.att-definition,
        mml.att-encoding
    }
