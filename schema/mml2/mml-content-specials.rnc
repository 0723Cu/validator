mml.cspecial = mml.apply.qname | mml.reln.qname | mml.lambda.qname
mml.apply.qname =
    element apply { mml.ContentExpression, mml.Common.attrib }
mml.reln.qname =
    element reln { mml.ContentExpression, mml.Common.attrib }
mml.lambda.qname =
    element lambda { mml.ContentExpression, mml.Common.attrib }
