## 
##     Clip
## 
##         clipPath
## 
##     This module declares markup to provide support for clipping.
##   
[ xml:lang = "en" ]
grammar {
    include "svg-basic-clip.rnc" inherit = ns1
    
    ## 
    ##       extend content model of clipPath
    ##     
    SVG.clipPath.class |= SVG.Shape.class | SVG.Text.class
}
